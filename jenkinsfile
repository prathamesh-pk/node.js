pipeline{
    agent{ label 'jdk11'}
    stages{
        stage('source managment'){
            steps{
                git branch: 'main', 
                url: 'https://github.com/Azure-Samples/js-e2e-express-server.git'
            }
        }
        stage('shellscript'){
            steps{
                sh '''sudo apt update
sudo apt install npm -y
npm install
npm run build
npm pack
npm test'''

        
            }
        }
       stage('diployer'){
            steps{
                rtNpmDeployer(
                    id: 'tiger',
                    serverId: 'jfrog',
                    repo: 'practice'
                )
            }
        }
         stage ('Exec npm publish') {
            steps {
                rtNpmPublish (
//                    tool: 'npm', // Tool name from Jenkins configuration
                    deployerId: "tiger"
                )
            }
        }
    }
}